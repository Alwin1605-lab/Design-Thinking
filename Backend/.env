# GramaFix Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Groq API Key for Whisper Speech-to-Text
# Get your free API key from: https://console.groq.com/keys
GROQ_API_KEY=

# MongoDB Configuration (already configured)
MONGODB_URI=mongodb://localhost:27017/GramaFix

# Optional: JWT Secret for authentication (generate a random string)
JWT_SECRET=your_random_secret_key_here_12345

# Optional: Email notifications
SENDGRID_API_KEY=

# Optional: SMS notifications
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
